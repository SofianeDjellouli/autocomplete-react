{
  "name": "@sofiane-d/autocomplete-react",
  "version": "1.0.0-rc.4",
  "description": "A customizable React autocomplete component",
  "author": "Sofiane Djellouli",
  "license": "ISC",
  "homepage": "https://autocomplete-react.firebaseapp.com",
  "bugs": "https://github.com/SofianeDjellouli/autocomplete-react/issues",
  "keywords": [
    "enhanced input",
    "react",
    "autocomplete",
    "autosuggest",
    "typeahead",
    "dropdown",
    "select",
    "combobox",
    "omnibox",
    "accessibility",
    "WAI-ARIA",
    "multiselect",
    "multiple selection"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SofianeDjellouli/autocomplete-react.git"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "clean": "nwb clean-module && nwb clean-demo",
    "build": "nwb build-react-component --copy-files",
    "prepublishOnly": "npm run build",
    "deploy": "npm publish --access=public",
    "start": "nwb serve-react-demo",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "preversion": "npm test",
    "version": "npm run build && git add -A dist",
    "postversion": "git push && git push --tags && rm -rf build/temp"
  },
  "dependencies": {
    "lodash.escaperegexp": "4.x",
    "prop-types": "15.x",
    "react-uid": "2.x"
  },
  "peerDependencies": {
    "react": ">=0.14.9"
  },
  "devDependencies": {
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "nwb": "0.24.x",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
